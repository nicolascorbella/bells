{
  "version": 2,
  "routes": [
    {
      "src": "/",
      "dest": "/client/index.html"
    },
    {
      "src": "/client/tienda",
      "dest": "/tienda.html"
    },
    {
      "src": "/client/carrito",
      "dest": "/carrito.html"
    },
    {
      "src": "/(.*)",
      "dest": "/server/main.js"
    }
  ]
}
