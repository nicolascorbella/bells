{
  "version": 2,
  "builds": [
    {
      "src": "server/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)", 
      "headers": { "Location": "https://theguardianbell.com.ar/$1" }, 
      "status": 301
    },
    {
      "src": "/create_preference",
      "methods": ["POST"],
      "dest": "/server/main.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server/main.js"
    }
  ]
}
