{
  "version": 2,
  "routes": [
    {
      "src": "/tienda/productos",
      "dest": "/api/productos.json"
    },
    {
      "src": "/tienda",
      "dest": "/api/tienda.html"
    },
    {
      "src": "/carrito",
      "dest": "/api/carrito.html"
    },
    {
      "src": "/(.*)",
      "dest": "/api/main.js"
    }
  ]
}
